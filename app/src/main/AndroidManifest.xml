<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hpnotebook.volunteerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".SearchEventsActivity" />
        <activity android:name=".EventsNearbyActivity" />
        <activity android:name=".EventDetailActivity" />
        <activity android:name=".OrgProfileActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".AddEventActivity"></activity>
    </application>

</manifest>